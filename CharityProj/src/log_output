function filterByKey() {
	var keyfield=document.keyform.keyword_list.value;
	var location="http://www.charitynavigator.org/index.cfm?Submit2=GO&bay=search.results&keyword_list="+escape(keyfield);
	if(keyfield) 
	this.location.href=location;
}

